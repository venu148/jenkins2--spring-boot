pipeline{
    agent{
        node{
        label 'slave_node1'
        }
    }
    options { 
  buildDiscarder(logRotator( numToKeepStr: '5', daysToKeepStr: '5'))
}
triggers {
  pollSCM '*/5 * * * *'
}
    stages{
    stage('Initialize') {
            steps {
                script {
                    properties([pipelineTriggers([pollSCM('')])])
                }
                git 'https://github.com/venu148/jenkins2--spring-boot.git'
                }
        }
        stage('1'){
            steps{
                sh 'echo hii11'
                sh 'sleep 10'
        }
    }
    }
}
