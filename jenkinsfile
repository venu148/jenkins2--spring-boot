        pollSCM 'H/5 * * * *'
    }
    stages{
    stage('Initialize') {
            steps {
                git 'https://github.com/venu148/jenkins2--spring-boot.git'
                }
        }
        stage('1'){
            steps{
                sh 'echo hii11'
                sh 'sleep 10'
        }
    }
    }
}
